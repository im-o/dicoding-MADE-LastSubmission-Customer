<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stimednp.boxofficefavoritecuk">

    <uses-permission android:name="com.stimednp.aplikasimoviecataloguesub4.READ_DATABASE" />
    <uses-permission android:name="com.stimednp.aplikasimoviecataloguesub4.WRITE_DATABASE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_primary_green_moviedb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_primary_green_moviedb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".myactivity.DetailsMovieActivity"></activity>
        <activity android:name=".myactivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>